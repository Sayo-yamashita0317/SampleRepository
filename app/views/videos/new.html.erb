videos#new

<%= form_for @video do |f| %>
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title, :size => 140 %>
  </div>
  <div class="field">
    <%= f.label :youtube_url %>
    <%= f.text_field :youtube_url, :size => 140 %>
  </div>
  <div class="field">
    <%= f.label :date %>
    <%= f.date_field :date %>
  </div>
  <%= f.submit "動画を投稿する" %>
<% end %>
<%= link_to "動画一覧に戻る", videos_path %>