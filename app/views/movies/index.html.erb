<p>動画投稿一覧</p>
<%= @movies.each do |m| %>
  <%= m.title %>
  <% unless m.video.blank? %>
   <%= video_tag m.video_url ,controls: true, muted: true %>
  <% end %>
  <p>場所</p>
  <%= m.url  %>
  <iframe src="https://www.google.com/maps/embed?<%= m.url%>" width="600" height="450" frameborder="0" style="border:0;" allowfullscreen="" aria-hidden="false" tabindex="0"></iframe>
  <%= link_to "削除する", movie_path(m.id), method: :delete %>
<% end %>

<%= link_to "トップページへ", videos_path %>